<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">

	<servlet>
		<servlet-name>GestionProfilServlet</servlet-name>
		<servlet-class>com.images.GestionProfilServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GestionProfilServlet</servlet-name>
		<url-pattern>/gereProfil</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>RechercheProfilServlet</servlet-name>
		<servlet-class>com.images.RechercheProfilServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RechercheProfilServlet</servlet-name>
		<url-pattern>/rechercheProfil</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>TacheDeFondServlet</servlet-name>
		<servlet-class>com.images.TacheDeFondServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>TacheDeFondServlet</servlet-name>
		<url-pattern>/tacheDeFond</url-pattern>
	</servlet-mapping>
		
	<welcome-file-list>
		<welcome-file>profil.html</welcome-file>
	</welcome-file-list>
</web-app>
